import pandas as pd
import numpy as np
import matplotlib.pyplot as plt
from math import sqrt

# Read file
df = pd.read_csv("45a38dad-9dc8-4b99-860f-f1e6c3eb92c5 (1).csv")

# Clean column names
df.columns = df.columns.str.strip()

print("Columns:", df.columns)

# Rename first two columns safely
df.rename(columns={
    df.columns[0]: "Year",
    df.columns[1]: "Cyclonic_Total"
}, inplace=True)

# Extract data
years = df["Year"]
cyclones = df["Cyclonic_Total"]

n = len(cyclones)

# Hypothesis test
mu0 = 10
z_critical = 1.645

sample_mean = np.mean(cyclones)
sample_std = np.std(cyclones, ddof=1)

z_stat = (sample_mean - mu0) / (sample_std / sqrt(n))

print("Z-Statistic:", round(z_stat,3))
print("Critical Z:", z_critical)

if z_stat > z_critical:
    print("Reject H0")
else:
    print("Fail to Reject H0")

# Plot
plt.figure(figsize=(8,5))
plt.plot(years, cyclones)
plt.xlabel("Year")
plt.ylabel("Cyclonic Disturbance Total")
plt.title("Cyclonic Disturbance Over Years")
plt.grid(True)
plt.show()


